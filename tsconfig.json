{
  "compilerOptions": {
    "moduleResolution": "node",
    "declaration": true,
    "esModuleInterop": true,
    "jsx": "react",
    "strict": true,
    "noImplicitOverride": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true
  },
  "include": ["src/index.ts"]
}
