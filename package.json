{
  "name": "react-titled",
  "version": "1.0.1",
  "description": "To set document.title with ease. Nested. Flexible.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "browserslist": "last 2 versions, ie 11",
  "files": [
    "*.md",
    "LICENSE",
    "lib",
    "es"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tajo/react-titled"
  },
  "author": "Vojtech Miksu <vojtech@miksu.cz>",
  "license": "MIT",
  "scripts": {
    "build": "npm run clean && npm run build-es && npm run build-cjs",
    "build-es": "tsc --module es2015 --target es2015 --outDir ./es -p tsconfig.json",
    "build-cjs": "tsc --module commonjs --target es5 --outDir ./lib -p tsconfig.json",
    "build:examples": "cross-env vite build",
    "clean": "rimraf es lib build",
    "prepublish": "cross-env NODE_ENV=production npm run build",
    "lint": "eslint vite.config.js src examples --ext ts,tsx,js",
    "format": "prettier --write \"**/*.js\" \"**/*{.ts,.tsx}\"",
    "test": "cross-env vitest",
    "react": "enzyme-adapter-react-install 16"
  },
  "tags": [
    "react"
  ],
  "keywords": [
    "react",
    "react-component",
    "title",
    "document.title",
    "react-titled",
    "react-title",
    "nested"
  ],
  "dependencies": {},
  "devDependencies": {
    "@testing-library/react": "^12.1.2",
    "@types/react": "^17.0.38",
    "@typescript-eslint/parser": "^5.10.1",
    "@vitejs/plugin-react": "^1.1.4",
    "cross-env": "^5.0.1",
    "eslint": "^4.3.0",
    "eslint-plugin-prettier": "^2.1.2",
    "jsdom": "^19.0.0",
    "prettier": "^1.5.3",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "rimraf": "^2.5.0",
    "typescript": "^4.5.5",
    "vite": "^2.7.13",
    "vitest": "^0.2.5"
  },
  "peerDependencies": {
    "react": "^16.8.0-0 || ^17.0.0-0"
  }
}
